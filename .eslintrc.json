{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "curly": [
      "error",
      "all"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    // "sort-imports": [
    //   "error"
    // ],
    // "sort-keys": [
    //   "error"
    // ],
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "space-in-parens": [
      "error",
      "always"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
